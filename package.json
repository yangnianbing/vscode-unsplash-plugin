{
	"name": "image-find",
	"displayName": "image-find",
	"description": "关键词搜索图片，图片源unsplash",
	"publisher": "yangnianbing",
  "repository": "https://github.com/yangnianbing/vscode-unsplash-plugin",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.55.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "image-find.open-search",
				"title": "Image-Find:search picture from unsplash"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "cd webview && npm run build && cd ../ && webpack --mode production",
		"webpack": "webpack --mode production",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.55.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"ts-loader": "^8.1.0",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0",
		"webpack": "4.44.2",
		"webpack-cli": "^4.6.0"
	}
}
